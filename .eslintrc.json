{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-unused-vars": "warn",
    "no-console": "off",
    "class-methods-use-this": "warn",
    "no-plusplus": "off"
  }
}
